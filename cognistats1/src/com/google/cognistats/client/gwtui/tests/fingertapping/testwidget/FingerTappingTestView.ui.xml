<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:multitrial="urn:import:com.google.cognistats.client.gwtui.tests.multitrial.testwidget">
	<ui:style>

	</ui:style>
	<g:LayoutPanel width="100%" height="100%">
		<g:layer left="0px" width="100.0pct" top="0px" height="100.0pct">
		<multitrial:MultitrialTestView height="100%" width="100%" ui:field="multiTrialTest" />
		</g:layer>
		<g:layer left="10.0pct" width="80.0pct" top="20.0pct" height="16.0pct">
			<g:Label text="Press (space) as quickly as you can." horizontalAlignment="ALIGN_CENTER" ui:field="tappingLabel"/>
		</g:layer>
		<g:layer left="10.0pct" width="80.0pct" top="70.0pct" height="20.0pct">
			<g:FlowPanel width="100%" height="100%" ui:field="timeAndCount">
				<g:Label width="40%" height="50%" horizontalAlignment="ALIGN_CENTER" ui:field="timeLabel" />
				<g:Label width="40%" height="50%" horizontalAlignment="ALIGN_CENTER" ui:field="countLabel"/>
			</g:FlowPanel>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 