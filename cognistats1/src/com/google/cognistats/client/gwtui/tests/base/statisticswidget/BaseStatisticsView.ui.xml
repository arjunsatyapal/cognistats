<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:LayoutPanel>
		<g:layer left="0px" width="60px" top="0px" height="18px">
			<g:Label text="Statistics"/>
		</g:layer>
		<g:layer left="0px" width="100.0pct" top="24px" height="100.0pct">
			<g:Grid width="100%" height="100%">
				<g:row>
					<g:customCell>
						<g:Label text="Statistic"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Current session"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="All time"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Time"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="0:00:00" ui:field="labelTimeCurrentSession"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="0:00:00" ui:field="labelTimeAllTime"/>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 