<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:LayoutPanel width="100%" height="100%" ui:field="testResult">
      <g:layer left="10%" width="20%" top="50px" height="35px">
        <g:Label text="Attempts" />
      </g:layer>
      <g:layer left="35%" width="60%" top="50px" height="35px">
        <g:TextBox enabled="false" ui:field="textAttempts" />
      </g:layer>

      <g:layer left="10%" width="20%" top="100px" height="35px">
        <g:Label text="Success Ratio" />
      </g:layer>
      <g:layer left="35%" width="60%" top="100px" height="35px">
        <g:TextBox enabled="false" ui:field="textSuccessRatio" />
      </g:layer>

      <g:layer left="10%" width="20%" top="150px" height="35px">
        <g:Label text="Failure Count" />
      </g:layer>
      <g:layer left="35%" width="60%" top="150px" height="35px">
        <g:TextBox enabled="false" ui:field="textFailureCount" />
      </g:layer>

      <g:layer left="10%" width="20%" top="200px" height="35px">
        <g:Label text="Average Time(s)" />
      </g:layer>
      <g:layer left="35%" width="60%" top="200px" height="35px">
        <g:TextBox enabled="false" ui:field="textAverageTime" />
      </g:layer>

      <g:layer left="10%" width="20%" top="250px" height="35px">
        <g:Label text="Std Dev" />
      </g:layer>
      <g:layer left="35%" width="60%" top="250px" height="35px">
        <g:TextBox enabled="false" ui:field="textStdDev" />
      </g:layer>

      <g:layer left="10%" width="20%" top="300px" height="35px">
        <g:Label text="History" />
      </g:layer>
      <g:layer left="35%" width="60%" top="300px" height="30%">
        <g:TextArea enabled="false" ui:field="textAreaHistory" />
      </g:layer>
    </g:LayoutPanel>
</ui:UiBinder>
