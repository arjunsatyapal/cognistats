<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:LayoutPanel width="800px" height="600px">
    <g:layer left="5%" width="60%" top="5%" height="80%">
      <g:LayoutPanel width="100%" height="100%" ui:field="layoutPanelTest">
      </g:LayoutPanel>
    </g:layer>
    <g:layer left="70%" width="25%" top="5%" height="80%">
      <g:LayoutPanel width="100%" height="100%" ui:field="layoutPanelResult">
      </g:LayoutPanel>
    </g:layer>
    <g:layer left="5%" width="90px" top="90%" height="29px">
      <g:Button text="Start" ui:field="buttonStart" />
    </g:layer>
    <g:layer right="5%" width="90px" top="90%" height="29px">
      <g:Button text="Stop" ui:field="buttonStop" />
    </g:layer>
  </g:LayoutPanel>
</ui:UiBinder> 