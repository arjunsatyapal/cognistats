<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:FocusPanel ui:field="focusPanel">
    <g:DockLayoutPanel width="100%" height="100%">
      <g:north size="113.0">
        <g:VerticalPanel width="100%" height="100%">
          <g:Image url="images/cognistats_logo.jpg" width="300px"
            height="72px" />
          <g:HorizontalPanel width="100%" height="29px">
            <g:LayoutPanel width="100%" height="100%">
              <g:layer right="0px" width="194px" top="0px" height="30px">
                <g:MenuBar>
                  <g:MenuItem text="Login" ui:field="menuItemLoginLogout" />
                  <g:MenuItem text="Test Selector" ui:field="menuItemTestSelector"/>
                </g:MenuBar>
              </g:layer>
              <g:layer right="194px" width="173px" top="10px" height="20px">
                <g:Label ui:field="labelUserId" horizontalAlignment="ALIGN_RIGHT" />
              </g:layer>
            </g:LayoutPanel>
          </g:HorizontalPanel>
        </g:VerticalPanel>
      </g:north>
      <g:center size="100.0">
        <g:LayoutPanel width="100%" height="100%" ui:field="workspace">
          <g:layer left="0px" width="100%" top="0px" height="100%">
            <g:LayoutPanel width="100%" height="100%">
              <g:layer left="50%" width="115px" top="50%" height="29px">
                <g:Button text="Test Selector" ui:field="buttonTestSelector" />
              </g:layer>
            </g:LayoutPanel>
          </g:layer>
        </g:LayoutPanel>
      </g:center>
    </g:DockLayoutPanel>
  </g:FocusPanel>
</ui:UiBinder> 
